{
  "java.configuration.updateBuildConfiguration": "automatic",
  "protoc": {
    "options": ["-I=./"]
  },
  "[java]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.inlayHints.enabled": "off"
  },
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "[markdown]": {
